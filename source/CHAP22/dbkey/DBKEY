//COBNRWI  PROC SYSOUT='*',                                             00100000
//         MEMBER=DUMMY,                                                00110000
//         WORK=VIO,                                                    00120000
//         ADV=NOADV,                                                   00130000
//         DATA='DATA(31)',                                             00140000
//         DYNAM='DYNAM',                                               00150000
