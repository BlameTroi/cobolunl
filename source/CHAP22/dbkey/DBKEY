//         COBRET=5,                                                    00170000
//         MAP=MAP,OFFSET=OFFSET                                        00180000
//*                                                                   * 00190000
//*                                                                     00200000
//COB      EXEC PGM=IGYCRCTL,REGION=2M,                                 00210000
//         PARM=('NOC(S),FLAG(I,E),TRUNC(BIN),XREF(FULL),NOSSR',        00220000
